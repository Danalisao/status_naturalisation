{
  "manifest_version": 3,
  "name": "Statut API du Dossier Naturalisation",
  "version": "2.0.7",
  "description": "Statut API du Dossier Naturalisation",
  "browser_specific_settings": {
    "gecko": {
      "id": "statut-naturalisation@extension.fr",
      "strict_min_version": "109.0"
    }
  },
  "permissions": [
    "scripting",
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "notifications",
    "storage"
  ],
  "host_permissions": [
    "https://administration-etrangers-en-france.interieur.gouv.fr/*"
  ],
  "content_scripts": [{
    "matches": ["https://administration-etrangers-en-france.interieur.gouv.fr/*"],
    "js": ["inject.js"],
    "run_at": "document_end"
  }],
  "web_accessible_resources": [{
    "resources": ["content.js", "crypto-js.min.js", "forge.min.js"],
    "matches": ["https://administration-etrangers-en-france.interieur.gouv.fr/*"]
  }],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "Statut API du Dossier Naturalisation"
  }
}
